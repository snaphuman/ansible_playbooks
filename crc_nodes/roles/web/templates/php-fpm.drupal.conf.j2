[drupal]
listen = /var/run/php-fpm/php7.0-fpm.sock
listen.owner = nginx
listen.group = nginx
listen.mode = 0660
user = webmaster
group = webmaster
pm = dynamic
pm.max_children = 10
pm.start_servers = 1
pm.min_spare_servers = 1
pm.max_spare_servers = 3
pm.max_requests = 500
security.limit_extensions = .php .php3 .php4 .php5 .php7 .html .htm .js .css .jpg .jpeg .gif .png .txt .ico .ogg .wav .mp3 .mp4 .json
